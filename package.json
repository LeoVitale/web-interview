{
  "name": "web-interview",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.13"
  },
  "scripts": {
    "start": "yarn server & react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test:coverage": "npm run test -- --coverage",
    "precommit": "lint-staged",
    "server": "yarn json-server --watch ./data/data.json --port 3011",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma es5 --no-semi --write",
      "eslint --quiet",
      "git add"
    ]
  },
  "dependencies": {
    "json-server": "^0.14.2",
    "node-sass": "^4.11.0",
    "normalize.css": "^8.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.10.0",
    "enzyme-to-json": "^3.3.5",
    "jest-enzyme": "^7.0.1",
    "prop-types": "^15.7.2",
    "react-hot-loader": "^4.7.1",
    "react-scripts": "^2.1.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
